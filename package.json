{
  "name": "diagram-ai",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "AI-powered diagram generation tool with Mermaid and PlantUML support",
  "author": "DiagramAI Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "clean": "rm -rf .next node_modules/.cache",
    "rebuild": "npm run clean && npm run dev",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "db:init": "node scripts/init-db.js",
    "db:seed": "node scripts/seed-db.js",
    "dead-code": "knip",
    "dead-code:exports": "knip --exports",
    "dead-code:deps": "depcheck",
    "ci": "npm run format:check && npm run lint && npm run type-check && npm run dead-code",
    "ci:full": "npm run format:check && npm run lint && npm run type-check && npm run dead-code && npm run build",
    "precommit": "npm run format:check && npm run lint && npm run type-check"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.23",
    "@ai-sdk/google": "^2.0.17",
    "@ai-sdk/openai": "^2.0.42",
    "@ai-sdk/openai-compatible": "^1.0.19",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/language": "^6.11.3",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.4",
    "@hookform/resolvers": "^3.10.0",
    "@lezer/highlight": "^1.2.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@tailwindcss/postcss": "^4.1.14",
    "@uiw/codemirror-themes": "^4.25.2",
    "@uiw/react-codemirror": "^4.25.2",
    "ai": "^5.0.60",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.4.0",
    "canvg": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "file-saver": "^2.0.5",
    "jose": "^6.1.0",
    "lucide-react": "^0.544.0",
    "mermaid": "^11.12.0",
    "next": "^15.3.0",
    "pako": "^2.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.64.0",
    "react-zoom-pan-pinch": "^3.7.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.17.0",
    "@types/pako": "^2.0.4",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "autoprefixer": "^10.4.20",
    "depcheck": "^1.4.7",
    "eslint": "^9.20.0",
    "eslint-config-next": "^15.3.0",
    "knip": "^5.65.0",
    "postcss": "^8.4.50",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.0"
  }
}
